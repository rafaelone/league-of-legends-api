{
  "name": "league-of-legends-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "npm run env:load tsx watch src/main.ts",
    "env:load": "dotenv -e ./.env --",
    "db:migrate": "npm run env:load prisma migrate dev",
    "db:studio": "npm run env:load prisma studio",
    "db:seed": "npm run env:load prisma db seed",
    "db:init": "npm run env:load prisma init",
    "test": "dotenv -e ./.env.test -- jest --coverage --watchAll false",
    "upload-champions-thumbnail": "tsx src/scripts/upload-champions-thumbnail.ts",
    "create-champions": "tsx src/scripts/create-champions.ts",
    "env:load-test": "dotenv -e ./.env.test --",
    "db:studio-test": "npm run env:load-test prisma studio",
    "db:migrate-test": "npm run env:load-test prisma migrate dev",
    "create-champions-test": "npm run env:load-test tsx src/scripts/create-champions.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/redis": "^5.0.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@prisma/client": "^5.19.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "ioredis": "^5.4.1",
    "minio": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@jest/globals": "^29.7.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^7.4.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.3",
    "typescript": "^5.6.2"
  }
}